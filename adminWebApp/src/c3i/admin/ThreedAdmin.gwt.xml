<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.4/distro-source/core/src/gwt-module.dtd">


<module>


    <inherits name="com.google.gwt.core.Core"/>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.user.theme.chrome.Chrome"/>
    <inherits name="com.google.gwt.rpc.RPC"/>

    <inherits name="smartsoft.util.Util"/>
    <inherits name="c3i.core.ThreedCore"/>
    <inherits name="c3i.smartClient.SmartClient"/>
    <!--<inherits name="c3i.skin.previewPanel.PreviewPanel"/>-->
    <!--<inherits name="c3i.skin.summaryPanel.SummaryPanel"/>-->
    <inherits name="c3i.jpgGen.JpgGen"/>
    <inherits name="c3i.repo.Repo"/>

    <inherits name="com.google.common.collect.Collect"/>

    <inherits name='com.google.gwt.debug.Debug'/>

    <stylesheet src="../index.css"/>


    <entry-point class='c3i.admin.client.MainEntryPoint'/>

    <source path="client"/>
    <source path="shared"/>

    <public path="public"/>


    <inherits name="com.google.gwt.logging.Logging"/>
    <!--<set-property name="gwt.logging.enabled" value="FALSE"/> # To disable logging-->

    <set-property name="gwt.logging.logLevel" value="WARNING"/>
    # To change the default logLevel

    <set-property name="gwt.logging.consoleHandler" value="ENABLED"/>
    # To disable a default Handler
    <set-property name="gwt.logging.developmentModeHandler" value="ENABLED"/>
    <set-property name="gwt.logging.systemHandler" value="ENABLED"/>
    <set-property name="gwt.logging.firebugHandler" value="ENABLED"/>

    <set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED"/>
    <set-property name="gwt.logging.popupHandler" value="DISABLED"/>

    <inherits name="com.google.gwt.json.JSON"/>
    <inherits name="smartsoft.util.Util"/>
    <inherits name='org.timepedia.exporter.Exporter'/>
    <inherits name="com.google.common.collect.Collect"/>


</module>